#!/usr/bin/expect -f
#
# 26_test_fedtkb_inplace.exp - Test if FEDTKB.ODL works from original location
#

set timeout 300
set port 2322

log_user 1

spawn telnet localhost $port

sleep 3
send "\r"

expect {
    "User:" { }
    "Today's date?" {
        send "1-JAN-92\r"
        expect "Current time?"
        send "12:00\r"
        expect "Start timesharing?"
        send "Y\r"
        expect "Proceed with system startup?"
        send "\r"
        expect "User:"
    }
    timeout { puts ">>> Timeout"; exit 1 }
}

send "1,2\r"
expect "Password:"
send "SYSTEM\r"
expect {
    "Job number to attach to?" { send "\r"; expect -re {\$ } }
    -re {\$ } { }
}

puts "\n>>> Logged in"

# Check FEDTKB.ODL
puts "\n>>> Checking FEDTKB.ODL details..."
send "DIR/FULL SY:\[0,200\]FEDTKB.ODL\r"
expect -re {\$ }

# Test TKB with FEDTKB.ODL - complete the build
puts "\n>>> Testing complete TKB build with FEDTKB.ODL..."
send "TKB\r"
expect "TKB>"

# This should create a task based on FEDTKB.ODL
# Output goes to [1,2]TESTFED.TSK
send "SY:TESTFED/FP=SY:\[0,200\]FEDTKB.ODL\r"

expect {
    "TKB>" {
        puts ">>> Command accepted"
    }
    "TKB -- *FATAL*" {
        puts ">>> FATAL error on FEDTKB.ODL from original location!"
        expect -re {.*\n}
    }
    "TKB -- *" {
        puts ">>> TKB message"
        exp_continue
    }
    timeout { puts ">>> Timeout" }
}

# Enter options and try to complete build
send "/\r"
expect {
    "Enter Options:" {
        puts ">>> Options mode"
        # Try to complete
        send "//\r"
    }
    "TKB>" {
        send "//\r"
    }
}

expect {
    -re {\$ } {
        puts ">>> Back to DCL"
    }
    "TKB -- *FATAL*-File" {
        puts ">>> FATAL - illegal format from original location too!"
        expect -re {.*\n}
        expect -re {\$ }
    }
    "TKB -- *FATAL*" {
        puts ">>> Other FATAL error"
        expect -re {.*\n}
        expect -re {\$ }
    }
    "TKB -- *DIAG*" {
        puts ">>> TKB diagnostic"
        exp_continue
    }
    "undefined" {
        puts ">>> Undefined symbols (expected if we're missing object files)"
        exp_continue
    }
    "TKB>" {
        send "//\r"
        exp_continue
    }
    timeout { puts ">>> Timeout" }
}

# Check what was created
send "DIR SY:TESTFED.*\r"
expect -re {\$ }

send "BYE\r"
expect eof

puts "\n>>> Test complete"
