[File DP2:[200,1]ADVSHT.SUB copied to TT:[1,70]??????.???]
1       SUB ADVINI &

2       COMMON ACC$(10%)=7%,AR$=80%,C%,C$=10%,CRLF$=2%, &
        FLAG%(10%),HP%(10%),LEVEL%(10%),KB%(10%),NAM$(10%)=30%,NO.OF.USERS%, &
        XP.(10%),STUN(10%), &
        NPCMESS$(10%)=40%,OB$(10%,9%)=30%,PASS$(10%)=10%, &
        ROOM%(10%),SENT.KB%,SENT.MESS$=80%,SENT.PROG%,SENT.PROJ%, &
        USER%,CI$=1%,WEAPON$(10%)=20%,WEAPON%(10%), &
        ATT.LEVEL%(10%),DEF.LEVEL%(10%),MON.HP%(10%),MON.DAM%(10%), &
        SPEC$(10%)=6%,MON.ROOM%(10%),MON.XP%(10%),RUN.ACC$=9%,MMM$=128%, &
        BLIND%(10%),USER1%,SPY%(70%),FATIGUE%(10%),READFLG%(10%), &
        BFF$=64%,COM$=64%,PLAYERS%,TURN%,FORCE% &
\       ON ERROR GOTO 25000 &
\	MAX.PLAYERS%=10% &
\	MAIN.KEYBOARD%=1% &
\	PLAYERS%=0% &

10	J$=SYS(CHR$(12%)) &
\       RUN.ACC$="["+NUM1$(ASCII(RIGHT(J$,6%)))+","+NUM1$(ASCII(RIGHT(J$,5%))) 
