1       SUB ADVSHT(MESS$) &

3	SUBEXIT IF FLAG%(USER%) AND 2% &

4	FOR I%=1% TO MAX.PLAYERS% &
\	IF ROOM%(I%)=ROOM%(USER%) AND I%<>USER% THEN &
	PRINT #1%, RECORD 32767%+1%+KB%(I%),MESS$ &
\	PRINT #1%, RECORD 32767%+1%+SPY%(KB%(I%)),MESS$ IF SPY%(KB%(I%)) &

10	NEXT I% &
\	SUBEXIT &

25000	! ERRORS       &

29000   OPEN "[100,0]LOG100.5" AS FILE #4%, MODE 2% &
\       PRINT #4%, 'ADVSHT : ';ERT$(ERR);' at line';ERL;' at ';TIME$(0%) &
\       PRINT #4%, 'Message was ';MESS$;' for room ';ROOM%(USER%);' from ' &
