15110   DEF FNCLEAR$ &
\	IF FLAG%(USER%) AND 1% THEN &
	HP%(USER%)=HP%(USER%)*3%/4% &
\	FLAG%(USER%)=FLAG%(USER%)-1% &

15120   FNEND &

15130   DEF FNSHOUT$(MMESS$) &
\       PRINT #1%, RECORD 32767%+1%+KB%(JJJ%), CRLF$;MMESS$ IF ROOM%(JJJ%)= &
        ROOM%(USER%) AND JJJ%<>USER% &
        FOR JJJ%=1% TO 8% UNLESS FLAG%(USER%) AND 2% &
\       FNEND &

15140   DEF FNGMD%(R%) &
\       GOTO 15150 IF MON.ROOM%(MON%)=R% FOR MON%=0% TO 10% &
\       MON%=-1% &
\       MON%=I% IF MON.ROOM%(I%)=0% FOR I%=0% TO 10% &
\       MON%=INT(RND*10%) IF MON%=-1% &
\       JJJJJJ%=FNSMD%(MON%) IF MON.ROOM%(MON%) &
\       J$=MONSTER$(R%) &
\       ATT.LEVEL%(MON%)=CVT$%(J$) &
\       J$=RIGHT(J$,3%) &
\       DEF.LEVEL%(MON%)=CVT$%(J$) &
\       J$=RIGHT(J$,3%) &
\       MON.HP%(MON%)=CVT$%(J$) &
\       J$=RIGHT(J$,3%) &
\       MON.DAM%(MON%)=CVT$%(J$) &
\       J$=RIGHT(J$,3%) &
\       SPEC$(MON%)=LEFT(J$,6%) &
\       J$=RIGHT(J$,7%) &
\       MON.XP%(MON%)=CVT$%(J$) &
\       J$=RIGHT(J$,3%) &
\       MON.FLAG%(MON%)=CVT$%(J$) &
\       MON.ROOM%(MON%)=R% &

15150   FNEND &

15160   DEF FNSMD%(R%) &
\       J$=CVT%$(ATT.LEVEL%(R%))+CVT%$(DEF.LEVEL%(R%))+CVT%$(MON.HP%(R%))+ &
        CVT%$(MON.DAM%(R%))+ &
        SPEC$(R%)+CVT%$(MON.XP%(R%))+CVT%$(MON.FLAG%(R%))+CVT%$(0%) &
\       MONSTER$(MON.ROOM%(R%))=J$ &
\       MON.ROOM%(R%)=0% &
\       FNEND &

15170   DEF FNSTRIP$(J$) &
\       JJ$="$~*@" &
\       J$=LEFT(J$,INSTR(1%,J$,MID(JJ$,QQ%,1%))-1%) &
        IF INSTR(1%,J$,MID$(JJ$,QQ%,1%)) FOR QQ%=1% TO LEN(JJ$) &
\       FNSTRIP$=J$ &
\       FNEND &

15180   DEF FNVAL%(J$) &
\       JJ$="$~*@" &
\       QQ%=0% &
\       QQ%=QQ% OR INSTR(1%,J$,MID(JJ$,QU%,1%)) &
        FOR QU%=1% TO LEN(JJ$) &
\       FNVAL%=QQ% &
\       FNEND &

