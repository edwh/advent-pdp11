\       IF LEFT(SW.ARG$,LEN(J$))<>J$ OR LEN(J$)=0% THEN &
        CALL ADVOUT(KB%(USER%),CRLF$+"You can't do that.") UNLESS NO.LOG% &
\       SUBEXIT &

