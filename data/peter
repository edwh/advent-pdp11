USER%)<60% &
\	USER1%=0% IF LEVEL%(I%)>30% AND LEVEL%(USER%)<=30% &
\       SUBEXIT &

11000   CALL ADVOUT(KB%(USER%),CRLF$+"I can't see him.") &
        UNLESS INSTR(1%,CVT$$(PEOPLE$,255%),NAM$) &
\       SUBEXIT &

